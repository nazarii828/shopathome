{
  "routes": [
    {
      "route": "/login",
      "serve": "/.auth/login/github"
    },
    {
      "route": "/logout",
      "serve": "/.auth/logout"
    },
    {
      "route": "/.auth/login/twitter",
      "statusCode": "404"
    },
    {
      "route": "/api/products",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/api/admin",
      "allowedRoles": ["authenticated", "admin"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ]
}
